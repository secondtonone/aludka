{
  "name": "server",
  "version": "1.2.15",
  "description": "Server for app",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node --trace-deprecation --env-file=../.env dist/server.js",
    "run": "pnpm build && pnpm start",
    "type-check": "tsc --noEmit",
    "dev": "tsx watch --env-file=../.env ./src/server.ts",
    "scripts:transactions": "tsx --env-file=../.env ./src/scripts/transactionsToDb.ts",
    "scripts:info": "tsx --env-file=../.env ./src/scripts/contractInfoToDb.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/postgres": "^5.2.2",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "fastify-type-provider-zod": "^2.1.0"
  },
  "devDependencies": {
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.1.0",
    "@scalar/fastify-api-reference": "^1.25.28",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "json-schema-to-ts": "^3.1.1",
    "pino-pretty": "^11.2.2"
  }
}
