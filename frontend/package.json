{
  "name": "frontend",
  "version": "0.0.8",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "format": "prettier --config ./.prettierrc -w '**/*.{tsx,ts}'",
    "preview": "vite preview"
  },
  "dependencies": {
    "@eslint/js": "^9.2.0",
    "@rive-app/react-canvas": "^4.17.5",
    "@rollup/plugin-inject": "^5.0.5",
    "@tanstack/react-query": "^5.62.11",
    "@telegram-apps/sdk-react": "^2.0.10",
    "@telegram-apps/telegram-ui": "^2.1.8",
    "@ton-api/client": "^0.2.0",
    "@ton-community/assets-sdk": "^0.0.5",
    "@ton/core": "^0.59.0",
    "@tonconnect/ui-react": "^2.0.9",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.20",
    "db": "workspace:*",
    "eruda": "^3.0.1",
    "globals": "^15.2.0",
    "i18next": "^24.0.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "maskdata": "^1.3.3",
    "postcss": "^8.4.49",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.1.3",
    "react-router-dom": "^6.28.0",
    "tailwindcss": "^3.4.15",
    "ton-client": "workspace:*",
    "tonweb": "^0.0.66",
    "vite": "^5.2.11",
    "vite-plugin-mkcert": "^1.17.6",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier-plugin-tailwindcss": "^0.6.9"
  }
